<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.session.AdminRegistration">
   <children>
      <TextField fx:id="login" layoutX="225.0" layoutY="90.0" promptText="Логин" visible="true" />
      <TextField fx:id="password" layoutX="225.0" layoutY="145.0" promptText="Пароль" visible="true" />
      <RadioButton fx:id="student" layoutX="70.0" layoutY="222.0" mnemonicParsing="false" text="студент" visible="true">
         <toggleGroup>
            <ToggleGroup fx:id="group" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="teacher" layoutX="69.0" layoutY="254.0" mnemonicParsing="false" text="учитель" toggleGroup="$group" visible="true" />
      <RadioButton fx:id="zamDirector" layoutX="69.0" layoutY="286.0" mnemonicParsing="false" text="зам директора" toggleGroup="$group" visible="true" />
      <Button fx:id="regButton" layoutX="242.0" layoutY="303.0" mnemonicParsing="false" text="Зарегистрировать" visible="true" />
      <Button fx:id="button_logout" layoutX="275.0" layoutY="351.0" mnemonicParsing="false" onAction="#goBack" text="Назад" />
      <TextField fx:id="course_id" layoutX="151.0" layoutY="218.0" promptText="Course id" />
      <TextField fx:id="subject_id" layoutX="151.0" layoutY="250.0" promptText="Subject id" />
   </children>
</AnchorPane>
