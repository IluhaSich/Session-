<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.session.StudentMain">
   <children>
      <TableView fx:id="tbl_subjects" layoutX="163.0" layoutY="14.0" onMouseClicked="#getSelected" prefHeight="295.0" prefWidth="275.0">
        <columns>
          <TableColumn fx:id="t_id" prefWidth="56.0" text="id" />
          <TableColumn fx:id="t_subject" prefWidth="218.0" text="subject" />
        </columns>
      </TableView>
      <Button fx:id="btn_exam" layoutX="464.0" layoutY="352.0" mnemonicParsing="false" onAction="#pass_Exam" prefHeight="34.0" prefWidth="109.0" text="Сдать экзамен" />
      <Button fx:id="btn_back" layoutX="33.0" layoutY="357.0" mnemonicParsing="false" onAction="#goBack" text="Выйти" />
   </children>
</AnchorPane>
